{
  "name": "amplify-hope-website",
  "version": "1.0.1",
  "private": true,
  "license": "MIT",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "format": "prettier --write .",
    "start": "next start",
    "lint": "npm run lint:eslint && npm run lint:madge",
    "lint:eslint": "eslint . --ext ts --ext tsx --ext js",
    "lint:madge": "madge --circular --ts-config tsconfig.json --extensions ts,js",
    "prepare": "husky install",
    "type-check": "tsc --pretty --noEmit"
  },
  "lint-staged": {
    "*.@(ts|tsx)": [
      "npm run lint",
      "npm run format"
    ]
  },
  "dependencies": {
    "@prisma/client": "^3.9.2",
    "@stripe/react-stripe-js": "^1.7.0",
    "@stripe/stripe-js": "^1.22.0",
    "formik": "^2.2.9",
    "madge": "^5.0.1",
    "micro": "^9.3.4",
    "next": "12.0.10",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-md": "^5.0.0",
    "rxjs": "^7.5.2",
    "stripe": "^8.201.0"
  },
  "devDependencies": {
    "@types/node": "17.0.14",
    "@types/react": "17.0.38",
    "@typescript-eslint/eslint-plugin": "^5.10.2",
    "@typescript-eslint/parser": "^5.10.2",
    "dotenv": "^16.0.0",
    "eslint": "^7.32.0",
    "eslint-config-next": "12.0.10",
    "eslint-config-prettier": "^8.3.0",
    "eslint-config-standard": "^16.0.3",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-promise": "^5.2.0",
    "eslint-plugin-react": "^7.28.0",
    "husky": "^7.0.4",
    "lint-staged": "^12.3.3",
    "prettier": "2.5.1",
    "prettier-eslint": "^13.0.0",
    "prettier-plugin-prisma": "^3.9.0",
    "prisma": "^3.9.2",
    "sass": "^1.49.7",
    "ts-node-dev": "^1.1.8",
    "tsconfig-paths": "^3.12.0",
    "typescript": "4.5.5"
  },
  "engines": {
    "node": "14.x"
  }
}
